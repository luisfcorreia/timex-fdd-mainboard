set -tmpdir "/tmp"
set -xsthdpdir "xst"
run
-ifn timex_mainboard.prj
-ifmt mixed
-ofn timex_mainboard
-ofmt NGC
-p xc9572xl-10-VQ64
-top timex_mainboard
-opt_mode Speed
-opt_level 1
-iuc NO
-keep_hierarchy No
-netlist_hierarchy As_Optimized
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case Maintain
-verilog2001 YES
-fsm_extract YES
-fsm_encoding Auto
-safe_implementation No
-mux_extract Yes
-resource_sharing YES
-iobuf YES
-equivalent_register_removal YES
